@model List<YQMVC.Models.UserInfos>
    @{
        ViewBag.Title = "用户信息显示";
        Layout = "~/Views/Shared/_ParentPage1.cshtml";
    }
    <!DOCTYPE html>
    <html lang="en">
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link rel="shortcut icon" href="~/MoBanJS/img/favicon.ico">
        <link href="http://fonts.googleapis.com/css?family=Roboto+Slab:400,300,100,700" rel="stylesheet">
        <link href="http://fonts.googleapis.com/css?family=Roboto:500,400italic,100,700italic,300,700,500italic,400" rel="stylesheet">
        <link href="~/MoBanJS/css/bootstrap.min.css" rel="stylesheet">
        <link href="~/MoBanJS/css/style.css" rel="stylesheet">
        <link href="~/MoBanJS/plugins/font-awesome/css/font-awesome.min.css" rel="stylesheet">
        <link href="~/MoBanJS/plugins/switchery/switchery.min.css" rel="stylesheet">
        <link href="~/MoBanJS/plugins/bootstrap-select/bootstrap-select.min.css" rel="stylesheet">
        <link href="~/MoBanJS/plugins/datatables/media/css/dataTables.bootstrap.css" rel="stylesheet">
        <link href="~/MoBanJS/plugins/datatables/extensions/Responsive/css/dataTables.responsive.css" rel="stylesheet">
        <link href="~/MoBanJS/css/demo/jasmine.css" rel="stylesheet">
        <link href="~/MoBanJS/plugins/pace/pace.min.css" rel="stylesheet">
        <script src="~/MoBanJS/plugins/pace/pace.min.js"></script>
    </head>
    <body>
        <div id="container" class="effect mainnav-sm navbar-fixed mainnav-fixed">
            <div class="boxed">
                <div id="content-container">
                    <div id="page-content">
                        <div class="panel">
                            <div class="panel-heading">
                                <h3 class="panel-title">用户信息</h3>
                            </div>
                            <div class="panel-body">
                                <table id="demo-dt-selection" class="table table-striped table-bordered">
                                    <thead>
                                        <tr>
                                            <th>姓名</th>
                                            <th>身份证号</th>
                                            <th class="min-tablet">手机号</th>
                                            <th class="min-tablet">性别</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var item in Model)
                                        {
                                            <tr>
                                                <td>@item.UserInfo_Name</td>
                                                <td>@item.ID_Num</td>
                                                <td>@item.Phone_Num</td>
                                                <td>@if (item.UserInfo_Sex == 0)
                                                {
                                                    @:男
                                                }
                                                else
                                                {
                                                    @:女
                                                }
                                                </td>
                                            </tr>
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <script src="~/MoBanJS/js/jquery-2.1.1.min.js"></script>
        <script src="~/MoBanJS/js/bootstrap.min.js"></script>
        <script src="~/MoBanJS/plugins/fast-click/fastclick.min.js"></script>
        <script src="~/MoBanJS/js/scripts.js"></script>
        <script src="~/MoBanJS/plugins/nanoscrollerjs/jquery.nanoscroller.min.js"></script>
        <script src="~/MoBanJS/plugins/metismenu/metismenu.min.js"></script>
        <script src="~/MoBanJS/plugins/switchery/switchery.min.js"></script>
        <script src="~/MoBanJS/plugins/bootstrap-select/bootstrap-select.min.js"></script>
        <script src="~/MoBanJS/plugins/datatables/media/js/jquery.dataTables.js"></script>
        <script src="~/MoBanJS/plugins/datatables/media/js/dataTables.bootstrap.js"></script>
        <script src="~/MoBanJS/plugins/datatables/extensions/Responsive/js/dataTables.responsive.min.js"></script>
        <script src="~/MoBanJS/plugins/screenfull/screenfull.js"></script>
        <script src="~/MoBanJS/js/demo/tables-datatables.js"></script>
    </body>
</html>
